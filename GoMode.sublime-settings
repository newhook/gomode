{
	// env is a map of GOPATH, GOROOT and PATH variables.
	// e.g "env": { "PATH": "$HOME/go/bin:$PATH" }
	"env": {
		"GOPATH": "$HOME/src/services:$HOME/src/services/.gopack/vendor",
		"PATH": "/usr/local/bin:$HOME/src/services/bin:$PATH"
